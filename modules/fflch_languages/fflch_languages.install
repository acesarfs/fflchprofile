<?php

/**
 * @file
 * Install, update and uninstall functions for the module.
 */

use Drupal\fflch_languages\LanguageHelper;

/**
 * Implements hook_install().
 */
function fflch_languages_install() {
  if (!\Drupal::service('config.installer')->isSyncing()) {
    \Drupal::classResolver(LanguageHelper::class)->doConfig();
  }
}
